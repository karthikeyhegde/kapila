<h4><%=@item.name%></h4>
<div id="trs" class="row-fluid">
   <div class="span12">
     <h5>Item Transactions</h5>
      

<table class="table zebra-striped table-condensed" style='font-size:12px;width:80%' align="center">
  <thead style="background-color:#993300;color:#FFFFFF">
    <th>Date</th>
    <th>Site</th>
    <th>Contact</th>
    <th>Number</th>
    <th>Price</th>
    <th>Amount</th>
    
  </thead>
  <tbody>
   <% i = 0
    @txn_items.each{|c|
     i+= 1 %>
    <%if i%2 == 0%>
       <tr>
    <%else%>
       <tr class="info">
    <%end%>
    <td><%=c.transaction.on_date%></td>
    <td><%=c.transaction.site_name%></td>
    <td><%=c.transaction.contact.name_subname%>
    <td><%=c.ui_number%></td>
    <td><%=c.price%></td>
    <td> <%=c.ui_amount%></td>
          
   </tr>
   <%}%>
   <tr class="warning">
    <td><b>Total</b></td>
    <td></td>
    <td></td>
    <td><%=@all_numbers%></td>
    <td></td>
    <td> <%=@all_sum%></td>
          
   </tr>
  </tbody>

</table>
   </div>
</div>